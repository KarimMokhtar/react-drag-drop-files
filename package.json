{
  "name": "react-drag-drop-files",
  "version": "2.3.10",
  "description": "Light React Drag & Drop files and images library styled by styled-components",
  "author": "Karim <karimmokhtar28@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "dist/react-drag-drop-files.cjs.js/react-drag-drop-files.js",
  "module": "dist/react-drag-drop-files.esm.js/react-drag-drop-files.js",
  "types": "dist/src/react-drag-drop-files.d.ts",
  "source": "dist/src/react-drag-drop-files.ts",
  "scripts": {
    "build": "node build.mjs && yarn tsc",
    "build-watch": "BUILD_WATCH=true node build.mjs",
    "start-playground": "cd playground && npm run start",
    "install-all": "yarn install && cd playground && npm i",
    "dev": "npm-run-all --parallel build-watch start-playground",
    "deploy:npm": "yarn run build && npm publish",
    "lint:check": "eslint --ext .ts --ext .tsx .",
    "lint:fix": "eslint --fix --ext .ts --ext .tsx .",
    "test": "vitest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "keywords": [
    "light-react",
    "react",
    "react-drag-drop",
    "react-select-file",
    "drag",
    "drop",
    "upload",
    "select",
    "files",
    "images",
    "styled-compomnets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/KarimMokhtar/react-drag-drop-files"
  },
  "homepage": "https://github.com/KarimMokhtar/react-drag-drop-files",
  "contributors": [
    "Karim Mokhtar <karimmokhtar28@gmail.com>"
  ],
  "readme": "README.md",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "browserslist": "^4.21.4",
    "esbuild": "^0.15.15",
    "esbuild-plugin-browserslist": "^0.5.2",
    "prop-types": "^15.8.1",
    "styled-components": "^5.3.6",
    "vite": "^3.2.4",
    "vitest": "^0.25.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-react": "^2.2.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "glob": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.3",
    "web-vitals": "^3.1.0"
  },
  "volta": {
    "node": "14.17.6",
    "yarn": "1.22.11"
  }
}
